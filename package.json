{
  "name": "filerobot-uploader",
  "version": "2.3.0",
  "main": "dist/index.js",
  "description": "The Filerobot Uploader is a multi-function Uploader that will make uploads super easy in your web sites and apps. It is fast to integrate allows end users to upload media, files and any assets via Filerobot's reverse CDN. Files are stored into scalable and flexible Cloud storage, optimised and delivered over CDN to your end users rocekt fast. Features include inline image editing, auto-tagging, auto-cropping and many more.",
  "author": "scaleflex",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/scaleflex/filerobot-uploader"
  },
  "homepage": "https://github.com/scaleflex/filerobot-uploader#readme",
  "keywords": [
    "uploader",
    "reverse CDN",
    "asset management",
    "file manager",
    "file uploader",
    "image management",
    "image compression",
    "image resizing",
    "image acceleration",
    "fast uploader",
    "file listing",
    "image tagging",
    "image editor"
  ],
  "dependencies": {
    "core-js": "^3.1.3",
    "cropperjs": "^1.5.1",
    "filerobot-image-editor": "^1.2.1",
    "lodash.debounce": "^4.0.8",
    "lodash.find": "^4.6.0",
    "mobile-detect": "^1.4.3",
    "pretty-bytes": "^5.2.0",
    "rc-progress": "^2.3.0",
    "react-color": "^2.17.3",
    "react-focus-lock": "^1.19.1",
    "react-i18nify": "^1.11.14",
    "react-load-script": "0.0.6",
    "react-loadable": "^5.5.0",
    "react-tagsinput": "^3.19.0",
    "react-toastr": "^3.0.0",
    "react-tooltip": "^3.10.0",
    "react-virtualized": "^9.21.1",
    "smartcrop": "^2.0.3",
    "styled-components": "^4.2.1"
  },
  "peerDependencies": {
    "axios": "^0.17.1",
    "react": "^16.3.0",
    "react-dom": "^16.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-jsx": "^7.2.0",
    "@babel/plugin-transform-arrow-functions": "^7.2.0",
    "@babel/plugin-transform-modules-commonjs": "^7.4.4",
    "@babel/plugin-transform-object-assign": "^7.2.0",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "axios": "^0.18.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "css-loader": "0.28.7",
    "gh-pages": "^2.0.1",
    "highlight.js": "^9.15.8",
    "html-webpack-plugin": "^3.2.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-hot-loader": "^3.1.3",
    "style-loader": "0.19.0",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.4.1"
  },
  "scripts": {
    "start-demo-js": "webpack-dev-server --mode development --config config/webpack.demo-js-config.js",
    "start-demo-react": "webpack-dev-server --mode development --config config/webpack.demo-react-config.js",
    "clean-demo-js": "rm -rf examples/js-plugin/dist",
    "build-demo-js": "npm run clean-demo-js && webpack --mode production --config config/webpack.demo-js-config.js",
    "clean-demo-react": "rm -rf examples/react-plugin/dist",
    "build-demo-react": "npm run clean-demo-react && webpack --mode production --config config/webpack.demo-react-config.js",
    "clean-build": "rm -rf build",
    "build-new": "webpack --mode production --config config/webpack.js-config.js",
    "build-latest": "webpack --mode production --config config/webpack.js-config.js --env.latest",
    "build": "npm run clean-build && npm run build-new && npm run build-latest",
    "clean-dist": "rm -rf dist",
    "dist": "npm run clean-dist && npx babel projects/react-plugin --out-dir dist --copy-files",
    "deploy": "gh-pages -d examples/js-plugin/dist",
    "publish-demo": "npm run build-demo-js && npm run deploy"
  }
}
