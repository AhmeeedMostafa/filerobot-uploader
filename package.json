  "scripts": {
    "start-js": "webpack-dev-server --mode development --config webpack.demo-js-config.js",
    "start-react": "webpack-dev-server --mode development --config webpack.demo-react-config.js",
    "build": "rm -rf build && webpack --mode production --config webpack.js-config.js",
    "build-demo-js": "webpack --mode production --config webpack.demo-js-config.js",
    "build-demo-react": "webpack --mode production --config webpack.demo-react-config.js",
    "babeldist": "NODE_ENV=production babel projects/react-plugin --presets=env,react,stage-0 --out-dir dist --copy-files",
    "dist": "npm run clean && npm run babeldist",
    "clean": "rm -rf dist",
    "test": "node scripts/test.js --env=jsdom"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
